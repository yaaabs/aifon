{
  "expo": {
    "name": "Aifon Calculator",
    "slug": "aifon-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.aifon.calculator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aifon.calculator"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "config": {
        "firebase": {
          "analyticsId": ""
        }
      }
    }
  }
}
